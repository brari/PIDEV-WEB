                    </div>
                    <ul class="nav navliste" id="nav">
                        {% if (app.user) %}
                            <li class="active"><a href="{{ path('patisserie_homepage',{'idp':app.user.id}) }}" class="active">Home</a></li>
                        {% else %}
                            <li class="active"><a href="{{ path('patisserie_homepage',{'idp':1}) }}" class="active">Home</a></li>
                        {% endif %}
                        <a href="{{ path('client_homepage') }}">Toutes les Patisseries</a>
